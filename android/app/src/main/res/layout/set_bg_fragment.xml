<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:paddingTop="5dp"
    android:paddingBottom="15dp"
    tools:context=".setBoardFragment.Set_bg_Fragment">
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">
            <RelativeLayout
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:layout_margin="3dp"
                android:id="@+id/bg01RL"
                android:padding="1dp"
                android:layout_width="0dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:background="@color/bg_select_menu"
                    android:padding="5dp"
                    android:layout_height="match_parent">
                    <View
                        android:layout_width="match_parent"
                        android:id="@+id/bg01"
                        android:layout_height="match_parent"
                        android:background="@color/setboardbgcolor01" />
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:id="@+id/bg02RL"
                android:layout_margin="3dp"
                android:padding="1dp"
                android:layout_width="0dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:background="@color/bg_select_menu"
                    android:padding="5dp"
                    android:layout_height="match_parent">

                    <View
                        android:id="@+id/bg02"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/setboardbgcolor02" />
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:layout_margin="3dp"
                android:id="@+id/bg03RL"
                android:padding="1dp"
                android:layout_width="0dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:background="@color/bg_select_menu"
                    android:padding="5dp"
                    android:layout_height="match_parent">
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/bg03"
                        android:background="@color/setboardbgcolor03" />
                </RelativeLayout>
            </RelativeLayout>
        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">
            <RelativeLayout
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:id="@+id/bg04RL"
                android:layout_margin="3dp"
                android:padding="1dp"
                android:layout_width="0dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:background="@color/bg_select_menu"
                    android:padding="5dp"
                    android:layout_height="match_parent">
                    <View
                        android:layout_width="match_parent"
                        android:id="@+id/bg04"
                        android:layout_height="match_parent"
                        android:background="@color/setboardbgcolor04" />
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:layout_margin="3dp"
                android:padding="1dp"
                android:id="@+id/bg05RL"
                android:layout_width="0dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:background="@color/bg_select_menu"
                    android:padding="5dp"
                    android:layout_height="match_parent">
                    <View
                        android:layout_width="match_parent"
                        android:id="@+id/bg05"
                        android:layout_height="match_parent"
                        android:background="@color/setboardbgcolor05" />
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:layout_margin="3dp"
                android:id="@+id/bg06RL"
                android:padding="1dp"
                android:layout_width="0dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:background="@color/bg_select_menu"
                    android:padding="5dp"
                    android:layout_height="match_parent">
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:id="@+id/bg06"
                        android:background="@color/setboardbgcolor06" />
                </RelativeLayout>
            </RelativeLayout>
        </TableRow>
    </TableLayout>
</FrameLayout>